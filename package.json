    "axios": "^0.21.1",
    "date-fns": "^2.24.0",
    "firebase": "8.2.3",
    "global": "^4.4.0",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-agora": "^3.4.6",
    "react-native-appstate-hook": "^1.0.6",
