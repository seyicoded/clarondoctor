{
  "name": "clarondocdoctor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 12'",
    "pods": "npx pod-install ios",
    "clean": "watchman watch-del-all",
    "start": "react-native start --reset-cache",
    "build-all": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "test": "jest",
    "lint": "eslint .",
    "codepush-ios": "appcenter codepush release-react -a claron/doctorAppiOS -d Production",
    "codepush-android": "appcenter codepush release-react -a claron/doctorAppAndroid -d Production",
    "codepush": "appcenter codepush release-react -a claron/doctorAppiOS -d Production && appcenter codepush release-react -a claron/doctorAppAndroid -d Production"
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.1",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-clipboard/clipboard": "^1.9.0",
